<!--pages/category/category.wxml-->

<!-- text使用 -->
<!-- 1.selectable: true -->
<!-- 默认情况下text中的文本长按不能选中 -->
<text>pages/category/category.wxml \n</text>
<text selectable>你好小程序</text>

<!-- 2.space: 决定文本空格的大小 -->

<text>Hello World\n</text>
<text space="nbsp">Hello World\n</text>
<text space="emsp">Hello World\n</text>
<text space="ensp">Hello World\n</text>

<!-- 4.decode属性: 是否解码文本 -->
<text decode>5 &gt; 3</text>

<!-- button -->
<button hover-class="pressed">按钮</button>
<button size="mini">按钮</button>
<!-- type属性 -->
<button size="mini" type="warn" plain loading="{{isBtnLoading}}" bindtap="chBtnState">按钮</button>
